# Turnkey API Configuration Template
# Copy this file to .env.local and fill in your actual values

# Turnkey API Base URL (default production endpoint)
TURNKEY_API_BASE_URL=https://api.turnkey.com

# Your Turnkey Organization ID
# Find this in your Turnkey dashboard
TURNKEY_ORGANIZATION_ID=your_organization_id_here

# API Key Name
# The name of your API key (not the private key itself)
TURNKEY_API_KEY_NAME=your_api_key_name_here

# SECURITY NOTE: 
# DO NOT store your actual private key in this file
# The script will prompt for the private key at runtime for security

# Deletion Options
# Set to true to delete without export (WARNING: irreversible)
# Set to false to export data before deletion (recommended)
DELETE_WITHOUT_EXPORT=false